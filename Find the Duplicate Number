class Solution {
    public int findDuplicate(int[] nums) {
        int L=0,H=nums.length-1;
       while(L<H)
       {
           int m=(L+H)/2;
           int k=0;
           for (int num:nums) 
           {
                if(num<=m) 
                {
                    k++;
                }
            }
            if(k>m) 
            {
                H=m;
            } 
           else 
           {
             L=m+1;
           }
        }
        return L;
       }
}
