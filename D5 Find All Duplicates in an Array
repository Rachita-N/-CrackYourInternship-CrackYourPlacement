class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        int n=nums.length;
        ArrayList<Integer> arr1 = new ArrayList<Integer>(n);
       for(int i=0;i<n;i++)
        {
            int index=Math.abs(nums[i])-1;
            if(nums[index]<0) 
                arr1.add(index+1);
            else 
                nums[index]=-nums[index];
        }
        return arr1;
    }
}
