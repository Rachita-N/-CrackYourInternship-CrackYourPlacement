class Solution {
    public List<String> generateParenthesis(int n) 
    {
        ArrayList<String> ans = new ArrayList<>();
        generate(ans, "", n, n);
        return ans;
    }
    public void generate(ArrayList ans, String s, int l, int r)
    {
        if(l == 0 && r == 0)
        { 
            ans.add(s);
            return;
        }
        if(l > 0) generate(ans, s + "(", l - 1, r);
        if(r > l) generate(ans, s + ")", l, r - 1); 
    }
}
